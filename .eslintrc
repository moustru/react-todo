{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "jest": true
  },
  "rules": {
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off",
    "import/prefer-default-export": "off",
    "arrow-parens": "off",
    "array-bracket-spacing": "off",
    "no-plusplus": "off",
    "prefer-const": "off",
    "no-undef": "off",
    "import/no-unresolved": "off",
    "import/order": "off"
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}